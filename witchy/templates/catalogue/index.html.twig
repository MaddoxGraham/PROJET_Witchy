<div class="row bg-Ardoise">
  {% for produit in produits %}
    <div class="col-lg-3 mb-5 col-md-6">
      <div class="card" data-images="{{ produit.photos|map(p => p.src)|join(',') }}">
        {% set primaryPhoto = produit.photos|filter(p => p.isPrimary is not null and p.isPrimary)|first %}
        <div class="parentRelatif cardImgSeller">
          <a href="{{ path('app_products_details', {'slug':produit.slug}) }}">
            {% if primaryPhoto.src[:4] == 'http' %}
              <img src="{{ primaryPhoto.src }}" alt="{{ produit.shortLibel }}" class="d-block h-100 d-inline-block card-img-top img-fluid w-100">
            {% else %}
              <img src="{{ asset('assets/uploads/products/mini/300x300-' ~ primaryPhoto.src ) }}" alt="{{ produit.shortLibel }}" class="d-block h-100 d-inline-block card-img-top img-fluid w-100">
            {% endif %}
          </a>
          <button type="button" class="card-text LargeFont">Add to cauldron</button>
        </div>
        <div class="card-body">
          <div class="font-serif">{{ produit.shortLibel }}</div>
